<odoo>
    <data>
        <template id="management_bug_template">
            <t t-call="web.html_container">
                <t t-slot="title">Management Bug</t>
                <t t-slot="body">
                    <h1>Manage Bugs</h1>
                    <div>
                        <t t-foreach="bugs" t-as="bug">
                            <div>
                                <h2><t t-esc="bug.name"/></h2>
                                <p><t t-esc="bug.description"/></p>
                            </div>
                        </t>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>